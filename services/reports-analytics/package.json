{
  "name": "reports-analytics",
  "version": "0.1.1",
  "description": "TBD",
  "main": "index.js",
  "author": "Basaas GmbH",
  "license": "Apache-2.0",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "watch": "nodemon src/index.js",
    "coverage": "yarn test --coverage",
    "build:docker": "docker build -t openintegrationhub/reports-analytics:${VERSION} -f Dockerfile .",
    "start-dev-db": "yarn run-rs -v 4.0.12",
    "start-dev-rabbit": "docker run -d --hostname my-rabbit --name some-rabbit rabbitmq:3"
  },
  "dependencies": {
    "@basaas/node-logger": "1.1.5",
    "@openintegrationhub/event-bus": "1.1.3"
  },
  "devDependencies": {
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "get-port": "5.0.0",
    "jest": "24.7.1",
    "mongodb": "3.3.3",
    "mongodb-memory-server": "6.0.1",
    "nock": "11.6.0",
    "nodemon": "1.19.4",
    "run-rs": "0.6.2",
    "supertest": "4.0.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
      "js": "node --harmony"
    },
    "events": {
      "restart": ""
    },
    "watch": [
      "src/",
      "doc/"
    ],
    "ext": "js json yaml"
  }
}